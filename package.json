{
    "name": "docspace",
    "version": "1.0.0",
    "license": "ISC",
    "scripts": {
        "client": "cd client && cross-env NODE_PATH=./ yarn start",
        "client-build": "cd client && yarn build",
        "server": "node ./server/server.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "prod": "yarn client-build && yarn server",
        "pretest": "eslint --ignore-path .gitignore .",
        "precommit": "yarn pretest",
        "prepush": "yarn pretest"
    },
    "dependencies": {
        "babel-eslint": "^8.2.6",
        "bcrypt": "^3.0.0",
        "body-parser": "^1.18.3",
        "connect-mongo": "^2.0.1",
        "cookie-parser": "^1.4.3",
        "cross-env": "^5.2.0",
        "eslint-config-loopback": "^11.0.0",
        "express": "^4.16.2",
        "express-session": "^1.15.6",
        "i18next": "^11.6.0",
        "jodit-react": "^1.0.17",
        "jsonwebtoken": "^8.3.0",
        "mongoose": "^5.2.6",
        "nodemailer": "^4.6.8",
        "nodemailer-smtp-transport": "^2.7.4",
        "normalizr": "^3.2.4",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "passport-local-token": "^1.0.1",
        "path": "^0.12.7",
        "react-spinners": "^0.4.4"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.6",
        "concurrently": "^3.5.0",
        "eslint": "^5.3.0",
        "eslint-config-standard": "^11.0.0",
        "eslint-config-standard-react": "^6.0.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-node": "^7.0.1",
        "eslint-plugin-promise": "^3.8.0",
        "eslint-plugin-react": "^7.10.0",
        "eslint-plugin-standard": "^3.1.0",
        "husky": "^0.14.3"
    }
}
